{
    "name": "evgenii-gl/test_movavi",
    "type": "library",
    "description": "Test movavi",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gletchikov Evgeniy",
            "email": "krotkof@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.3",
        "guzzlehttp/guzzle": "~6.0",
        "phpdocumentor/phpdocumentor": "2.9.x-dev",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Course\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Course\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
